{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noEmitOnError": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "es5",
    "declaration": false,
    "lib": ["webworker", "scripthost"],
    "baseUrl": "../worker/src",
    "rootDir": "../worker/src",
    "outDir": "../src/worker"
  },
  "include": ["../worker/src/worker.ts", "../src/types.ts"],
  "exclude": ["node_modules"]
}
